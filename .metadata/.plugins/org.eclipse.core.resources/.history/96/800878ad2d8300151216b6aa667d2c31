package client;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class TempConvertTest {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		ApplicationContext ctx = (ApplicationContext) new ClassPathXmlApplicationContext(new  String[] {"WEB-INF/application-web-context.xml"});
		TempConvert tempConvert = (TempConvert)ctx.getBean("tempConvClient");
		System.out.println("Celcius to Fahrenheit--"+tempConvert.getTempConvertSoap().celsiusToFahrenheit("20"));
	}

}
